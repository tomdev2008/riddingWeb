<dwr>
	<allow>
		<create javascript="BackendBean" creator="spring">
			<param name="beanName" value="dwrBackendBean" />
		</create>
		<create javascript="RiddingBean" creator="spring">
			<param name="beanName" value="dwrRiddingBean" />
		</create>
		<create javascript="RiddingShareBean" creator="spring">
			<param name="beanName" value="dwrRiddingShareBean" />
		</create>
		<create javascript="RiddingPubBean" creator="spring">
			<param name="beanName" value="dwrRiddingPubBean" />
		</create>
        <convert converter="bean" match="com.ridding.meta.vo.Location" />
		<convert converter="bean" match="com.ridding.meta.vo.Location" />
		<convert converter="bean" match="net.sf.json.JSONArray" />
		<convert converter="bean" match="com.ridding.meta.vo.ProfileVO" />
		<convert converter="bean" match="com.ridding.meta.Profile" />


	</allow>
