public class RepostMap {
	/**
	 * id
	 */
	private long id;
	/**
	 * 微博id
	 */
	private long weiboId;
	/**
	 * 转发的微博id
	 */
	private long respostWeiBoId;
	/**
	 * 转发的微博用户的微博id
	 */
	private long respostSourceId;
	/**
	 * 转发的微博用户的用户id
	 */
	private long respostUserId;
	/**
	 * 资源类型
	 */
	private int sourceType;
	/**
	 * 创建时间
	 */
	private long createTime;

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public long getWeiboId() {
		return weiboId;
	}

	public void setWeiboId(long weiboId) {
		this.weiboId = weiboId;
	}

	public long getRespostWeiBoId() {
		return respostWeiBoId;
	}

	public void setRespostWeiBoId(long respostWeiBoId) {
		this.respostWeiBoId = respostWeiBoId;
	}

	public long getRespostSourceId() {
		return respostSourceId;
	}

	public void setRespostSourceId(long respostSourceId) {
		this.respostSourceId = respostSourceId;
	}

	public long getRespostUserId() {
		return respostUserId;
	}

	public void setRespostUserId(long respostUserId) {
		this.respostUserId = respostUserId;
	}

	public int getSourceType() {
		return sourceType;
	}

	public void setSourceType(int sourceType) {
		this.sourceType = sourceType;
	}

	public long getCreateTime() {
		return createTime;
	}

	public void setCreateTime(long createTime) {
		this.createTime = createTime;
	}

}
