package com.ridding.meta;

/**
 * @author zhengyisheng E-mail:zhengyisheng@gmail.com
 * @version CreateTime：2012-7-16 下午10:22:11 Class Description 城市信息表
 */
public class City {
	/**
	 * 城市id
	 */
	private long id;
	/**
	 * 城市名称
	 */
	private String name;
	/**
	 * 上级的id
	 */
	private long pid;
	/**
	 * 城市关系树
	 */
	private String tree;

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public long getPid() {
		return pid;
	}

	public void setPid(long pid) {
		this.pid = pid;
	}

	public String getTree() {
		return tree;
	}

	public void setTree(String tree) {
		this.tree = tree;
	}

}
